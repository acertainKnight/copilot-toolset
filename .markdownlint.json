{
  "default": true,
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "tables": false,
    "headings": false
  },
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "sub",
      "sup",
      "kbd",
      "img"
    ]
  },
  "MD041": false,
  "MD025": {
    "front_matter_title": ""
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD007": {
    "indent": 2
  },
  "MD029": {
    "style": "ordered"
  },
  "MD046": {
    "style": "fenced"
  }
}